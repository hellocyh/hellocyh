		<version>1.2.14</version>
	</dependency>
	
	<dependency>
	    <groupId>dubbo.parent</groupId>
    	<artifactId>dubbo.api</artifactId>
    	 <version>0.0.1-SNAPSHOT</version>
	</dependency>
	<dependency>  
	    <groupId>dubbo.parent</groupId>  
	    <artifactId>dubbo.console</artifactId>  
	    <version>0.0.1-SNAPSHOT</version>  
